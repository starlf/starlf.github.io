{
  "extends": "./src/.umi/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["./src/.umi/*"]
    },
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
}
